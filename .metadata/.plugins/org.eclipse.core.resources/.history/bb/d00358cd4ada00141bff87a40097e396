package interfaces;

import java.io.File;
import java.nio.file.NoSuchFileException;

import commands.NoSuchCommandException;

/**
 * Created by Ian on 30.03.2015.
 */
public interface Command {

    String keyWord();// возвращает ключивое слово типа "/copy"

    boolean check(String command, DirChanger dir) throws NoSuchFileException, NoSuchCommandException;//проверяет корректность строки
    //проверяет пути до файла, существует ли такой файл

    void execute(String command, DirChanger dirChanger);//выполняет

}
